@Library('Utils')_ 

pipeline {
    agent any
	tools {
        maven 'M3' 
    }
    stages {
        stage('Example') {
            steps {
				script { 
                 log.info 'Starting'
                 log.warning 'Nothing to do!'
				}
                echo 'Hello World'
            }
        }
    }
    post { 
        always { 
            echo 'I will always say Hello again!'
        }
    }
}