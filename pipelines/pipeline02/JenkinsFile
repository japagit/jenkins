@Library('Utils')_ 

pipeline {
    agent any
	tools {
        maven 'M3' 
    }
    stages {
        stage('Example') {
            steps {
				log.info 'Starting'
                echo 'Hello World'
            }
        }
    }
    post { 
        always { 
            echo 'I will always say Hello again!'
        }
    }
}